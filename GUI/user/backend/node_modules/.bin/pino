#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/tuan/Documents/Code/SQL/Project/football/GUI/user/backend/node_modules/pino/node_modules:/home/tuan/Documents/Code/SQL/Project/football/GUI/user/backend/node_modules:/home/tuan/Documents/Code/SQL/Project/football/GUI/user/node_modules:/home/tuan/Documents/Code/SQL/Project/football/GUI/node_modules:/home/tuan/Documents/Code/SQL/Project/football/node_modules:/home/tuan/Documents/Code/SQL/Project/node_modules:/home/tuan/Documents/Code/SQL/node_modules:/home/tuan/Documents/Code/node_modules:/home/tuan/Documents/node_modules:/home/tuan/node_modules:/home/node_modules:/node_modules:/home/tuan/Documents/Code/SQL/Project/football/GUI/user/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/tuan/Documents/Code/SQL/Project/football/GUI/user/backend/node_modules/pino/node_modules:/home/tuan/Documents/Code/SQL/Project/football/GUI/user/backend/node_modules:/home/tuan/Documents/Code/SQL/Project/football/GUI/user/node_modules:/home/tuan/Documents/Code/SQL/Project/football/GUI/node_modules:/home/tuan/Documents/Code/SQL/Project/football/node_modules:/home/tuan/Documents/Code/SQL/Project/node_modules:/home/tuan/Documents/Code/SQL/node_modules:/home/tuan/Documents/Code/node_modules:/home/tuan/Documents/node_modules:/home/tuan/node_modules:/home/node_modules:/node_modules:/home/tuan/Documents/Code/SQL/Project/football/GUI/user/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../pino/bin.js" "$@"
else
  exec node  "$basedir/../pino/bin.js" "$@"
fi
